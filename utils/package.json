{
  "name": "@anjianshi/utils",
  "version": "2.1.0",
  "description": "Common JavaScript Utils",
  "homepage": "https://github.com/anjianshi/js-packages/utils",
  "bugs": {
    "url": "https://github.com/anjianshi/js-packages/issues",
    "email": "anjianshi@gmail.com"
  },
  "license": "MIT",
  "author": "anjianshi <anjianshi@gmail.com>",
  "repository": "github:anjianshi/js-packages",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "directory": "dist"
  },
  "type": "module",
  "scripts": {
    "watch": "npm run clear && tsc --watch",
    "build": "npm run clear && tsc",
    "clear": "rm -rf dist",
    "prepublishOnly": "npm run build && node publish-prepare.cjs",
    "postpublish": "rm dist/package.json",
    "lint": "tsc --noEmit && eslint './src/**/*'"
  },
  "main": "index.js",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@anjianshi/presets-eslint-typescript": "workspace:../presets-eslint/typescript",
    "@anjianshi/presets-eslint-node": "workspace:../presets-eslint/node",
    "@anjianshi/presets-prettier": "workspace:../presets-prettier",
    "@anjianshi/presets-typescript": "workspace:../presets-typescript",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.12.7",
    "dotenv": "^16.4.5",
    "typeorm": "^0.3.20",
    "vconsole": "^3.15.1",
    "typescript": "^5.4.4"
  },
  "peerDependencies": {
    "chalk": "^5.3.0",
    "dayjs": "^1.11.10"
  },
  "peerDependenciesMeta": {
    "chalk": {
      "optional": true
    },
    "dayjs": {
      "optional": true
    }
  },
  "eslintIgnore": [],
  "prettier": "@anjianshi/presets-prettier/prettierrc"
}
